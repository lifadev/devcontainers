    "vscode": {
      "settings": {
        "go.toolsGopath": "/opt/go/vscode"
      },
      "extensions": ["golang.go"]
    },
    "manifest": {
      "dependencies": [
        {
          "name": "go",
          "version": "1.24.4",
          "hint": "tags/gov?[0-9]+\\.[0-9]+(\\.[0-9]+)?",
          "repository": "github.com/golang/go",
          "artifacts": [
            {
              "architecture": "amd64",
              "url": "https://go.dev/dl/go${VERSION}.linux-amd64.tar.gz",
              "checksum": "77e5da33bb72aeaef1ba4418b6fe511bc4d041873cbf82e5aa6318740df98717"
            },
            {
              "architecture": "arm64",
              "url": "https://go.dev/dl/go${VERSION}.linux-arm64.tar.gz",
              "checksum": "d5501ee5aca0f258d5fe9bfaed401958445014495dc115f202d43d5210b45241"
            }
          ]
        },
        {
          "name": "gotests",
          "version": "1.6.0",
          "hint": "tags/v?[0-9]+\\.[0-9]+(\\.[0-9]+)?",
          "repository": "github.com/cweill/gotests"
        },
        {
          "name": "gomodifytags",
          "version": "1.17.0",
          "hint": "tags/v?[0-9]+\\.[0-9]+(\\.[0-9]+)?",
