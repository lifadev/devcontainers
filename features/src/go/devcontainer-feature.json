{
  "id": "go",
  "name": "go",
  "version": "2025.6.2",
  "containerEnv": {
    "PATH": "/opt/go/bin:$PATH"
  },
  "customizations": {
    "vscode": {
      "settings": {
        "go.toolsGopath": "/opt/go/vscode"
      },
      "extensions": ["golang.go"]
    },
    "manifest": {
      "dependencies": [
        {
          "name": "go",
          "version": "1.24.3",
          "hint": "tags/gov?[0-9]+\\.[0-9]+(\\.[0-9]+)?",
          "repository": "github.com/golang/go",
          "artifacts": [
            {
              "architecture": "amd64",
              "url": "https://go.dev/dl/go${VERSION}.linux-amd64.tar.gz",
              "checksum": "3333f6ea53afa971e9078895eaa4ac7204a8c6b5c68c10e6bc9a33e8e391bdd8"
            },
            {
              "architecture": "arm64",
              "url": "https://go.dev/dl/go${VERSION}.linux-arm64.tar.gz",
              "checksum": "a463cb59382bd7ae7d8f4c68846e73c4d589f223c589ac76871b66811ded7836"
            }
          ]
        },
        {
          "name": "gotests",
          "version": "1.6.0",
          "hint": "tags/v?[0-9]+\\.[0-9]+(\\.[0-9]+)?",
          "repository": "github.com/cweill/gotests"
        },
        {
          "name": "gomodifytags",
          "version": "1.17.0",
          "hint": "tags/v?[0-9]+\\.[0-9]+(\\.[0-9]+)?",
          "repository": "github.com/fatih/gomodifytags"
        },
        {
          "name": "dlv",
          "version": "1.24.2",
          "hint": "tags/v?[0-9]+\\.[0-9]+(\\.[0-9]+)?",
          "repository": "github.com/go-delve/delve"
        },
        {
          "name": "goplay",
          "version": "1.0.0",
          "hint": "tags/v?[0-9]+\\.[0-9]+(\\.[0-9]+)?",
          "repository": "github.com/haya14busa/goplay"
        },
        {
          "name": "impl",
          "version": "1.4.0",
          "hint": "tags/v?[0-9]+\\.[0-9]+(\\.[0-9]+)?",
          "repository": "github.com/josharian/impl"
        },
        {
          "name": "gopls",
          "version": "0.18.1",
          "hint": "tags/gopls/v?[0-9]+\\.[0-9]+(\\.[0-9]+)?",
          "repository": "github.com/golang/tools"
        },
        {
          "name": "staticcheck",
          "version": "0.6.1",
          "hint": "tags/v?[0-9]{1,3}\\.[0-9]+(\\.[0-9]+)?",
          "repository": "github.com/dominikh/go-tools"
