      "settings": {
        "go.toolsGopath": "/opt/go/vscode"
      },
      "extensions": ["golang.go"]
    },
    "manifest": {
      "dependencies": [
        {
          "name": "go",
          "version": "1.25.2",
          "hint": "tags/gov?[0-9]+\\.[0-9]+(\\.[0-9]+)?",
