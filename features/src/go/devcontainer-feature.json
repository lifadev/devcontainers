            },
            {
              "architecture": "arm64",
              "url": "https://go.dev/dl/go${VERSION}.linux-arm64.tar.gz",
              "checksum": "65a3e34fb2126f55b34e1edfc709121660e1be2dee6bdf405fc399a63a95a87d"
            }
          ]
        },
        {
          "name": "gotests",
          "version": "1.6.0",
          "hint": "tags/v?[0-9]+\\.[0-9]+(\\.[0-9]+)?",
          "repository": "github.com/cweill/gotests"
        },
        {
          "name": "gomodifytags",
          "version": "1.17.0",
          "hint": "tags/v?[0-9]+\\.[0-9]+(\\.[0-9]+)?",
