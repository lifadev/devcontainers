{
  "id": "go",
  "name": "go",
  "version": "2025.10.15",
  "containerEnv": {
    "PATH": "/opt/go/bin:$PATH"
  },
  "customizations": {
    "vscode": {
      "settings": {
        "go.toolsGopath": "/opt/go/vscode"
      },
      "extensions": ["golang.go"]
    },
    "manifest": {
      "dependencies": [
        {
          "name": "go",
          "version": "1.25.3",
          "hint": "tags/gov?[0-9]+\\.[0-9]+(\\.[0-9]+)?",
          "repository": "github.com/golang/go",
          "artifacts": [
            {
              "architecture": "amd64",
              "url": "https://go.dev/dl/go${VERSION}.linux-amd64.tar.gz",
              "checksum": "0335f314b6e7bfe08c3d0cfaa7c19db961b7b99fb20be62b0a826c992ad14e0f"
            },
            {
              "architecture": "arm64",
              "url": "https://go.dev/dl/go${VERSION}.linux-arm64.tar.gz",
              "checksum": "1d42ebc84999b5e2069f5e31b67d6fc5d67308adad3e178d5a2ee2c9ff2001f5"
            }
          ]
        },
        {
          "name": "gotests",
          "version": "1.6.0",
          "hint": "tags/v?[0-9]+\\.[0-9]+(\\.[0-9]+)?",
          "repository": "github.com/cweill/gotests"
        },
        {
          "name": "gomodifytags",
          "version": "1.17.0",
          "hint": "tags/v?[0-9]+\\.[0-9]+(\\.[0-9]+)?",
