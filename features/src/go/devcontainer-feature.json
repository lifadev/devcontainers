          "artifacts": [
            {
              "architecture": "amd64",
              "url": "https://go.dev/dl/go${VERSION}.linux-amd64.tar.gz",
              "checksum": "d7fa7f8fbd16263aa2501d681b11f972a5fd8e811f7b10cb9b26d031a3d7454b"
            },
            {
              "architecture": "arm64",
              "url": "https://go.dev/dl/go${VERSION}.linux-arm64.tar.gz",
              "checksum": "9aaeb044bf8dbf50ca2fbf0edc5ebc98b90d5bda8c6b2911526df76f61232919"
            }
          ]
        },
        {
          "name": "gotests",
          "version": "1.6.0",
          "hint": "tags/v?[0-9]+\\.[0-9]+(\\.[0-9]+)?",
          "repository": "github.com/cweill/gotests"
        },
        {
          "name": "gomodifytags",
          "version": "1.17.0",
          "hint": "tags/v?[0-9]+\\.[0-9]+(\\.[0-9]+)?",
