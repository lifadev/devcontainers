{
  "id": "jupyter",
  "name": "jupyter",
  "version": "2025.11.14",
  "installsAfter": [
    "ghcr.io/lifadev/devcontainers/features/python313",
    "ghcr.io/lifadev/devcontainers/features/python314"
  ],
  "customizations": {
    "vscode": {
      "extensions": ["ms-toolsai.jupyter"]
    },
    "manifest": {
      "dependencies": [
        {
          "name": "ipykernel",
          "version": "7.1.0",
          "hint": "tags/v?[0-9]+\\.[0-9]+(\\.[0-9]+)?",
          "repository": "github.com/ipython/ipykernel"
        },
        {
          "name": "ipympl",
          "version": "0.9.8",
          "hint": "tags/v?[0-9]+\\.[0-9]+(\\.[0-9]+)?",
          "repository": "github.com/matplotlib/ipympl"
        },
        {
          "name": "matplotlib",
          "version": "3.10.8",
          "hint": "tags/v?[0-9]+\\.[0-9]+(\\.[0-9]+)?",
          "repository": "github.com/matplotlib/matplotlib"
        },
        {
          "name": "numpy",
          "version": "2.3.4",
          "hint": "tags/v?[0-9]+\\.[0-9]+(\\.[0-9]+)?",
          "repository": "github.com/numpy/numpy"
        }
      ]
    }
  }
}
